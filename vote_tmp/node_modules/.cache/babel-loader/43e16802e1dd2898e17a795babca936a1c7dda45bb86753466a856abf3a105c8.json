ollResult.vue';\n\nexport default {\n  data() {\n    return {\n      polls: [] // Хранит массив объектов опросов и результатов\n    };\n  },\n  components: {\n    VotePollForm,\n    VotePoll,\n    VotePollResult\n  },\n  methods: {\n    handlePollCreated(newPoll) {\n      // Добавляем новый опрос и инициализируем результаты\n      const initialResults = newPoll.options.reduce((acc, option) => {\n        acc[option] = 0;\n        return acc;\n      }, {});\n      this.polls.push({ poll: newPoll, results: initialResults });\n    },\n    handleVote(selectedOption, index) {\n      // Увеличиваем количество голосов для выбранного варианта в соответствующем опросе\n      this.polls[index].results[selectedOption]++;\n    }\n  }\n};\n</script>\n"],"mappings":";AAeA,OAAOA,YAAW,MAAO,+BAA+B;AACxD,OAAOC,QAAO,MAAO,2BAA2B;AAChD,OAAOC,cAAa,MAAO,iCAAiC;AAE5D,eAAe;EACbC,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,KAAK,EAAE,EAAC,CAAE;IACZ,CAAC;EACH,CAAC;EACDC,UAAU,EAAE;IACVL,YAAY;IACZC,QAAQ;IACRC;EACF,CAAC;EACDI,OAAO,EAAE;IACPC,iBAAiBA,CAACC,OAAO,EAAE;MACzB;MACA,MAAMC,cAAa,GAAID,OAAO,CAACE,OAAO,CAACC,MAAM,CAAC,CAACC,GAAG,EAAEC,MAAM,KAAK;QAC7DD,GAAG,CAACC,MAAM,IAAI,CAAC;QACf,OAAOD,GAAG;MACZ,CAAC,EAAE,CAAC,CAAC,CAAC;MACN,IAAI,CAACR,KAAK,CAACU,IAAI,CAAC;QAAEC,IAAI,EAAEP,OAAO;QAAEQ,OAAO,EAAEP;MAAe,CAAC,CAAC;IAC7D,CAAC;IACDQ,UAAUA,CAACC,cAAc,EAAEC,KAAK,EAAE;MAChC;MACA,IAAI,CAACf,KAAK,CAACe,KAAK,CAAC,CAACH,OAAO,CAACE,cAAc,CAAC,EAAE;IAC7C;EACF;AACF,CAAC","ignoreList":[]},"metadata":{},"sourceType":"module","externalDependencies":[]}